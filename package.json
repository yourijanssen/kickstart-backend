{
  "name": "heroes-back-end",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --watch",
    "start": "node build/index.js",
    "debug": "ts-node-dev --files --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" \"test/*.test.ts\" --quiet --fix",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" --write",
    "check": "npm run lint & npm run format",
    "setTestEnv": "set TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }'",
    "test": "npx mocha -r ts-node/register 'test/*.test.ts' --file test/mocha-setup.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "tsc-watch": "^6.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/express": "^4.17.17",
    "@types/mocha": "^9.1.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.3.2",
    "@types/regenerator-runtime": "^0.13.1",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^7.32.0",
    "mocha": "^9.2.2",
    "prettier": "^2.8.8",
    "regenerator-runtime": "^0.13.9",
    "ts-sinon": "^2.0.2",
    "supertest": "^6.1.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
